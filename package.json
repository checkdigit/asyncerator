{
  "name": "asyncerator",
  "version": "1.3.8",
  "description": "Provide supporting types for AsyncIterable/AsyncIterableIterators, promisified stream.pipeline implementation, and Array-like utility operators, sources and sinks.",
  "typings": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "prettier": "@checkdigit/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/checkdigit/asyncerator.git"
  },
  "engines": {
    "node": ">=16"
  },
  "author": "Check Digit, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/checkdigit/asyncerator/issues"
  },
  "homepage": "https://github.com/checkdigit/asyncerator#readme",
  "keywords": [
    "stream",
    "pipeline",
    "stream.pipeline",
    "AsyncIterator",
    "AsyncIterable",
    "AsyncIterableIterator"
  ],
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.161.0",
    "@aws-sdk/lib-storage": "^3.161.0",
    "@checkdigit/aws-nock": "^3.10.2",
    "@checkdigit/eslint-config": "^7.2.2",
    "@checkdigit/jest-config": "^1.0.1",
    "@checkdigit/prettier-config": "^3.0.0",
    "@checkdigit/typescript-config": "3.1.0",
    "@types/debug": "^4.1.7",
    "@types/uuid": "^8.3.4",
    "get-port": "^5.1.1",
    "openpgp": "^5.5.0",
    "rimraf": "^3.0.2",
    "uuid": "^8.3.2"
  },
  "jest": {
    "preset": "@checkdigit/jest-config"
  },
  "scripts": {
    "postversion": "git push && git push --tags",
    "lint": "eslint -f unix --ext .js,.ts src/**",
    "lint:fix": "eslint -f unix --ext .js,.ts src/** --fix",
    "prettier": "prettier --list-different .",
    "prettier:fix": "prettier --write .",
    "test": "npm run ci:compile && npm run ci:test && npm run ci:lint && npm run ci:style",
    "ci:compile": "tsc --noEmit",
    "ci:test": "jest --coverage=false",
    "ci:coverage": "jest --coverage=true",
    "ci:lint": "npm run lint",
    "ci:style": "npm run prettier"
  }
}
