{
  "name": "asyncerator",
  "version": "1.2.0",
  "description": "Provide supporting types for AsyncIterable/AsyncIterableIterators, promisified stream.pipeline implementation, and Array-like utility operators, sources and sinks.",
  "typings": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "prettier": "@checkdigit/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/checkdigit/asyncerator.git"
  },
  "engines": {
    "node": ">=14.16"
  },
  "author": "Check Digit, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/checkdigit/asyncerator/issues"
  },
  "homepage": "https://github.com/checkdigit/asyncerator#readme",
  "keywords": [
    "stream",
    "pipeline",
    "stream.pipeline",
    "AsyncIterator",
    "AsyncIterable",
    "AsyncIterableIterator"
  ],
  "dependencies": {
    "debug": "^4.3.1"
  },
  "devDependencies": {
    "@checkdigit/eslint-config": "^3.4.2",
    "@checkdigit/prettier-config": "^1.0.2",
    "@checkdigit/publish": "^1.0.19",
    "@checkdigit/typescript-config": "1.0.5",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/uuid": "^8.3.0",
    "eslint": "^7.25.0",
    "jest": "^26.6.3",
    "portfinder": "^1.0.28",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2"
  },
  "jest": {
    "collectCoverage": true,
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/?(*.)spec.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/build",
      "<rootDir>/coverage",
      "<rootDir>/node_modules"
    ]
  },
  "scripts": {
    "prepublishOnly": "publish",
    "postpublish": "rimraf dist .npmignore",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "lint": "eslint -f unix --ext .js,.ts src/**",
    "lint:fix": "eslint -f unix --ext .js,.ts src/** --fix",
    "prettier": "prettier --list-different src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts",
    "test": "tsc -noEmit && jest && npm run lint && npm run prettier"
  }
}
